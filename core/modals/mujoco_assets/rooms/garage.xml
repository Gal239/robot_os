<mujoco model="garage">
<visual>
  <headlight diffuse="0.6 0.6 0.6" ambient="0.3 0.3 0.3" specular="0 0 0"/>
  <rgba haze="0.15 0.25 0.35 1"/>
</visual>

<asset>
  <texture name="concrete" type="2d" file="../textures/concrete.png"/>
  <material name="floor_material" texture="concrete" texrepeat="8 8" specular="0.3" shininess="0.2"/>
  <texture name="concrete" type="2d" file="../textures/concrete.png"/>
  <material name="wall_material" texture="concrete" texrepeat="4 4" specular="0.05" shininess="0.01"/>
</asset>

<worldbody>
  <geom name="floor" type="plane" size="3.0 3.0 0.1" pos="0 0 0" material="floor_material" friction="1.0 0.05 0.01"/>
  <geom name="wall_north" type="box" size="3.0 0.15 1.5" pos="0 3.0 1.5" material="wall_material"/>
  <geom name="wall_south_left" type="box" size="0.25 0.15 1.5" pos="-2.75 -3.0 1.5" material="wall_material"/>
  <geom name="wall_south_right" type="box" size="0.25 0.15 1.5" pos="2.75 -3.0 1.5" material="wall_material"/>
  <!-- garage_door opening: 5m x 2.5m @ position 0m, interactive: no (just opening) -->
  <geom name="wall_east" type="box" size="0.15 3.0 1.5" pos="3.0 0 1.5" material="wall_material"/>
  <geom name="wall_west" type="box" size="0.15 3.0 1.5" pos="-3.0 0 1.5" material="wall_material"/>
</worldbody>
</mujoco>