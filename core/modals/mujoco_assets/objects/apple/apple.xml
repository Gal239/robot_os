<mujoco model="object">
    <!-- YCB Object: apple -->
    <asset>
        <mesh name="apple_mesh" file="textured.obj"/>
        <texture name="apple_tex" type="2d" file="texture_map.png"/>
        <material name="apple_mat" texture="apple_tex"/>
    </asset>

    <worldbody>
        <body name="apple">
            <geom type="mesh"
                  mesh="apple_mesh"
                  material="apple_mat"
                  mass="0.15"
                  friction="1 0.005 0.0001"/>
            <inertial pos="0 0 0" mass="0.15" diaginertia="0.001 0.001 0.001"/>
            <!-- fragile: True -->
        </body>
    </worldbody>
</mujoco>