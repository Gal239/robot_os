<mujoco model="object">
    <!-- YCB Object: chain -->
    <asset>
        <mesh name="chain_mesh" file="textured.obj"/>
        <texture name="chain_tex" type="2d" file="texture_map.png"/>
        <material name="chain_mat" texture="chain_tex"/>
    </asset>

    <worldbody>
        <body name="chain">
            <geom type="mesh"
                  mesh="chain_mesh"
                  material="chain_mat"
                  mass="0.5"
                  friction="1 0.005 0.0001"/>
            <inertial pos="0 0 0" mass="0.5" diaginertia="0.001 0.001 0.001"/>
            <!-- fragile: False -->
        </body>
    </worldbody>
</mujoco>