<mujoco model="object">
    <!-- YCB Object: orange -->
    <asset>
        <mesh name="orange_mesh" file="textured.obj"/>
        <texture name="orange_tex" type="2d" file="texture_map.png"/>
        <material name="orange_mat" texture="orange_tex"/>
    </asset>

    <worldbody>
        <body name="orange">
            <geom type="mesh"
                  mesh="orange_mesh"
                  material="orange_mat"
                  mass="0.2"
                  friction="1 0.005 0.0001"/>
            <inertial pos="0 0 0" mass="0.2" diaginertia="0.001 0.001 0.001"/>
            <!-- fragile: True -->
        </body>
    </worldbody>
</mujoco>