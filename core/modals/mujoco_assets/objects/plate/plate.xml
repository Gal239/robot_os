<mujoco model="object">
    <!-- YCB Object: plate -->
    <asset>
        <mesh name="plate_mesh" file="textured.obj"/>
        <texture name="plate_tex" type="2d" file="texture_map.png"/>
        <material name="plate_mat" texture="plate_tex"/>
    </asset>

    <worldbody>
        <body name="plate">
            <geom type="mesh"
                  mesh="plate_mesh"
                  material="plate_mat"
                  mass="0.4"
                  friction="1 0.005 0.0001"/>
            <inertial pos="0 0 0" mass="0.4" diaginertia="0.001 0.001 0.001"/>
            <!-- fragile: True -->
        </body>
    </worldbody>
</mujoco>